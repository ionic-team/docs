.breadcrumb {
  --token-separator-color: var(--c-indigo-40);
  --token-item-color: var(--c-indigo-60);
  --token-item-color-hover: var(--c-carbon-90);
  display: grid;
  align-items: center;
  grid-auto-flow: column;
  gap: 0.75rem;
  line-height: 1;
  font-size: 0.95rem;
  color: var(--token-separator-color);
}

.breadcrumb__logo svg {
  transition: color 0.2s;
  width: 24px;
  color: var(--token-item-color);
}

.breadcrumb__logo:hover svg {
  color: var(--token-item-color-hover);
}

.breadcrumb__item {
  transition: color 0.2s;
  color: var(--token-item-color);
  font-weight: 500;
  letter-spacing: -0.03em;
  font-size: 1.05rem;
  text-decoration: none;
  display: flex;
  align-items: center;
}

.breadcrumb__item:hover {
  text-decoration: none;
  color: var(--token-item-color-hover);
}

.breadcrumb__item--active,
.breadcrumb__item--active:hover {
  color: var(--token-heading-color-dark-hover);
}

.breadcrumb__item--appflow::before,
.breadcrumb__item--identity-vault::before,
.breadcrumb__item--auth-connect::before,
.breadcrumb__item--offline-storage::before,
.breadcrumb__item--premier-plugins::before {
  content: '';
  display: inline-block;
  width: 24px;
  height: 24px;
  background-size: cover;
  background-repeat: no-repeat;
  background-position: center center;
  margin-right: 0.5rem;
}

.breadcrumb__item--appflow::before {
  background-image: url('data:image/svg+xml,<svg fill="none" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 22 20"><circle cx="10.849" cy="13.961" r="2.453" fill="#4F68FF"/><path d="M21.699 20l-8.34-20H8.357l8.34 20h5.003z" fill="#4F68FF"/><path opacity=".2" d="M11.744 8.125L13.36 0H8.356l2.085 5 1.042 2.5.13.313.13.312z" fill="#000"/><path d="M0 20L8.34 0h5.003L5.004 20H.001z" fill="#639CFF"/></svg>');
